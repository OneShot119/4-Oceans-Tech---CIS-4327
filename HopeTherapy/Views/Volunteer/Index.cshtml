@model HopeTherapy.Models.Volunteer
   
@{
    ViewBag.Title = "Index";
}
<h2>Volunteer</h2>

<p> Please fill out the following information on the volunteer</p>

<hr />
@using (Html.BeginForm("Index", "Volunteer", FormMethod.Post))
{

    @Html.DisplayNameFor(i => i.FirstName) @Html.EditorFor(i => i.FirstName) @Html.ValidationMessageFor(i => i.FirstName) <br />
    @Html.DisplayNameFor(i => i.LastName) @Html.EditorFor(i => i.LastName) @Html.ValidationMessageFor(i => i.LastName) <br />
    @Html.DisplayNameFor(i => i.StreetAddress) @Html.EditorFor(i => i.StreetAddress) @Html.ValidationMessageFor(i => i.StreetAddress) <br />
    @Html.DisplayNameFor(i => i.City) @Html.EditorFor(i => i.City) @Html.ValidationMessageFor(i => i.City) <br />
    @Html.DisplayNameFor(i => i.ZipCode) @Html.EditorFor(i => i.ZipCode) @Html.ValidationMessageFor(i => i.ZipCode) <br />
    @Html.DisplayNameFor(i => i.County) @Html.EditorFor(i => i.County) @Html.ValidationMessageFor(i => i.County) <br />
    @Html.DisplayNameFor(i => i.CellPhoneNumber) @Html.EditorFor(i => i.CellPhoneNumber) @Html.ValidationMessageFor(i => i.CellPhoneNumber) <br />
    @Html.DisplayNameFor(i => i.HomePhoneNumber) @Html.EditorFor(i => i.HomePhoneNumber) @Html.ValidationMessageFor(i => i.HomePhoneNumber) <br />
    @Html.DisplayNameFor(i => i.OfficePhoneNumber) @Html.EditorFor(i => i.OfficePhoneNumber) @Html.ValidationMessageFor(i => i.OfficePhoneNumber) <br />
    @Html.DisplayNameFor(i => i.EmailAddress) @Html.EditorFor(i => i.EmailAddress) @Html.ValidationMessageFor(i => i.EmailAddress) <br />
    @Html.DisplayNameFor(i => i.CompanyName) @Html.EditorFor(i => i.CompanyName) @Html.ValidationMessageFor(i => i.CompanyName) <br />
    @Html.DisplayNameFor(i => i.Position) @Html.EditorFor(i => i.Position) @Html.ValidationMessageFor(i => i.Position) <br />
    @Html.DisplayNameFor(i => i.CompanyAddress) @Html.EditorFor(i => i.CompanyAddress) @Html.ValidationMessageFor(i => i.CompanyAddress) <br />
    @Html.DisplayNameFor(i => i.VolunteerDays) @Html.EditorFor(i => i.VolunteerDays) @Html.ValidationMessageFor(i => i.VolunteerDays) <br />
    @Html.DisplayNameFor(i => i.VolunteerPosition) @Html.EditorFor(i => i.VolunteerPosition) @Html.ValidationMessageFor(i => i.VolunteerPosition) <br />
    @Html.DisplayNameFor(i => i.HoursVolunteered) @Html.EditorFor(i => i.HoursVolunteered) @Html.ValidationMessageFor(i => i.HoursVolunteered) <br />
    @Html.DisplayNameFor(i => i.AreaOfInterest) @Html.EditorFor(i => i.AreaOfInterest) @Html.ValidationMessageFor(i => i.AreaOfInterest) <br />
    @Html.DisplayNameFor(i => i.Skills) @Html.EditorFor(i => i.Skills) @Html.ValidationMessageFor(i => i.Skills) <br />


    <input type="submit" value="Go" />
}


