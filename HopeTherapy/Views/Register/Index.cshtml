@model HopeTherapy.Models.Register

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p> You submitted this:</p>
@*<p> Username: @Model.Username</p>
<p> Password: @Model.Password</p>*@
<hr />

@using (Html.BeginForm("Index", "Register", FormMethod.Post))
{

    @Html.DisplayNameFor(i => i.Username) @Html.EditorFor(i => i.Username) @Html.ValidationMessageFor(i => i.Username)<br />
    @Html.DisplayNameFor(i => i.Password) @Html.EditorFor(i => i.Password) @Html.ValidationMessageFor(i => i.Password) <br />
    @Html.DisplayNameFor(i => i.Email) @Html.EditorFor(i => i.Email) @Html.ValidationMessageFor(i => i.Email) <br />
    @Html.DisplayNameFor(i => i.FirstName) @Html.EditorFor(i => i.FirstName) @Html.ValidationMessageFor(i => i.FirstName) <br />
    @Html.DisplayNameFor(i => i.LastName) @Html.EditorFor(i => i.LastName) @Html.ValidationMessageFor(i => i.LastName) <br />

    <input type="submit" value="Go" />
}